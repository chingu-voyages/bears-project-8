{
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true
	},
	"extends": ["airbnb-base", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": 2017,
		"sourceType": "module"
	},
	"rules": {
		"no-underscore-dangle": 0
	},
	"globals": {
		"expect": true,
		"describe": true,
		"test": true,
		"jest": true,
		"afterAll": true,
		"beforeAll": true
	}
}